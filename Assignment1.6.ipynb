   "source": [
    "#Create the below pattern using nested for loop in Python.\n",
    "\n",
    "#*\n",
    "#* *\n",
    "#* * *\n",
    "#* * * *\n",
    "#* * * * *\n",
    "#* * * *\n",
    "#* * *\n",
    "#* *\n",
    "#*"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "* \n",
      "* * \n",
      "* * * \n",
      "* * * * \n",
      "* * * * * \n",
      "* * * * \n",
      "* * * \n",
      "* * \n",
      "* \n"
     ]
    }
   ],
   "source": [
    "max=9\n",
    "for i in range(0,max):\n",
    "    if (i<=(max//2)):\n",
    "        print('* '*(i+1))\n",
    "    else:\n",
    "        print('* '*(max-i))\n",
    "\n",
    "        \n"
   ]
  }
